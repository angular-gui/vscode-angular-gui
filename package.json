{
  "name": "angular-gui",
  "displayName": "GUI for Angular",
  "description": "Run Angular CLI from your browser",
  "version": "0.1.3",
  "publisher": "sasxa-net",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-gui/vscode-angular-gui.git"
  },
  "icon": "angular-gui.png",
  "galleryBanner": {
    "color": "#FFC107",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Angular",
    "Angular CLI"
  ],
  "activationEvents": [
    "workspaceContains:**/.angular-cli.json"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.connectOffline",
        "title": "GUI for Angular: Start in Offline mode"
      },
      {
        "command": "extension.connectOnline",
        "title": "GUI for Angular: Start in Online mode"
      },
      {
        "command": "extension.rebuildConfiguration",
        "title": "GUI for Angular: Rebuild schematics"
      },
      {
        "command": "extension.disconnect",
        "title": "GUI for Angular: Stop"
      }
    ]
  },
  "scripts": {
    "build": "sh build.sh",
    "compile": "tsc -p ./",
    "debug": "webpack",
    "package": "vsce package",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "webpack",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/shelljs": "^0.7.5",
    "@types/socket.io": "^1.4.31",
    "awesome-typescript-loader": "^3.2.3",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.5.3",
    "vscode": "^1.1.5",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@angular-devkit/schematics": "0.0.35",
    "express": "^4.16.2",
    "ps-tree": "^1.1.0",
    "rxjs": "^5.5.2",
    "shelljs": "^0.7.8",
    "socket.io": "^2.0.4",
    "typescript": "^2.5.3",
    "stoppable": "^1.0.4"
  }
}